    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-pdf/renderer": "^4.3.0",
    "@types/pdfmake": "^0.2.11",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookies-next": "^6.1.0",
    "firebase": "^11.10.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.525.0",
    "next": "15.4.7",
